{
  "name": "suncoast-masons-gatsby",
  "description": "Gatsby Frontend replacement for https://suncoastmasons.wordpress.com/",
  "version": "0.1.0",
  "author": "TypeWorks <hellotypeworks@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://suncoastmasons.com"
  },
  "lint-staged": {
    "*.(js|jsx)": "yarn lint:fix"
  },
  "keywords": [
    "mason",
    "suncoast",
    "freemasons",
    "suncoastmasons",
    "florida"
  ],
  "scripts": {
    "start": "gatsby develop -H 0.0.0.0",
    "open": "gatsby develop -H 0.0.0.0 -o",
    "storybook": "start-storybook -p 9000 -c .storybook",
    "build": "gatsby build",
    "build:storybook": "build-storybook -c .storybook -o storybook",
    "build:all": "gatsby build && build-storybook -c .storybook -o storybook",
    "serve": "gatsby serve -o -p 8080",
    "clean": "gatsby clean",
    "lint": "eslint src -c .eslintrc.js --ext js,jsx",
    "lint:fix": "eslint src -c .eslintrc.js --fix --ext js,jsx",
    "prepare": "husky install"
  },
  "resolutions": {
    "graphql": "^15.4.0",
    "graphql-compose": "^7.25.0",
    "webpack": "^5.24.2"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.3",
    "@react-google-maps/api": "^2.7.0",
    "@sanity/block-content-to-react": "^3.0.0",
    "dotenv": "^10.0.0",
    "gatsby": "^4.5.4",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-gatsby-cloud": "^4.5.2",
    "gatsby-plugin-image": "^2.5.2",
    "gatsby-plugin-manifest": "^4.5.2",
    "gatsby-plugin-offline": "^5.5.2",
    "gatsby-plugin-react-helmet": "^5.5.0",
    "gatsby-plugin-root-import": "^2.0.8",
    "gatsby-plugin-sharp": "^4.5.2",
    "gatsby-plugin-styled-components": "^5.5.0",
    "gatsby-source-filesystem": "^4.5.2",
    "gatsby-source-sanity": "^7.3.2",
    "gatsby-transformer-sharp": "^4.5.0",
    "postmark": "^2.7.8",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-device-detect": "^2.1.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.25.0",
    "react-spring": "^9.4.2",
    "react-table": "^7.7.0",
    "styled-components": "^5.3.3",
    "twin.macro": "^2.8.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@emotion/babel-plugin": "^11.7.2",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addons": "^6.4.14",
    "@storybook/react": "^6.4.14",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "^10.4.2",
    "babel-plugin-styled-components": "^2.0.2",
    "babel-preset-gatsby": "^2.5.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-standard": "^5.0.0",
    "esm": "^3.2.25",
    "gatsby-plugin-postcss": "^5.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.2.2",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.15",
    "yarn-run-all": "^3.1.1"
  }
}
